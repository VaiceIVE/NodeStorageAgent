{
  "swagger": "2.0",
  "info": {
    "title": "Агент хранения необработанных файлов",
    "description": "Агент для хранения файлов, полученных от пользователей и со сторонних API написанный на NodeJS",
    "version": "1.0.0"
  },
  "host": "localhost:8005",
  "basePath": "/",
  "schemes": [
    "http"
  ],
  "paths": {
    "/upload": {
      "post": {
        "description": "Метод для загрузки одного файла в файловую систему",
        "consumes": [
          "multipart/form-data"
        ],
        "parameters": [
          {
            "name": "file",
            "in": "formData",
            "type": "file",
            "required": true,
            "description": "Входящий файл"
          }
        ],
        "responses": {
          "200": {
            "description": "Информация о названии файла в файловой системе хранилища",
            "schema": {
              "type": "string",
              "example": "FileAdressPlusDate.xlsx",
              "xml": {
                "name": "main"
              }
            }
          }
        }
      }
    },
    "/file/{filename}": {
      "get": {
        "description": "Метод для получения файла по названию из файловой системы хранилища",
        "consumes": [
          "multipart/form-data"
        ],
        "produces": [
          "attachment; filename="
        ],
        "parameters": [
          {
            "name": "filename",
            "in": "path",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "Файл, если есть",
            "schema": {
              "type": "string",
              "example": "FileAdressPlusDate.xlsx",
              "xml": {
                "name": "main"
              }
            }
          }
        }
      }
    },
    "/url": {
      "post": {
        "description": "",
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "schema": {
              "type": "object",
              "properties": {
                "url": {
                  "example": "any"
                },
                "types": {
                  "example": "any"
                }
              }
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/list": {
      "get": {
        "description": "",
        "parameters": [],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    }
  },
